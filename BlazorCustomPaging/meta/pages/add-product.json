{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Product Name",
                              "component": "ProductName",
                              "name": "ProductNameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 255,
                              "type": "textbox",
                              "name": "ProductName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${product.ProductName}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ProductNameRequiredValidator",
                              "component": "ProductName",
                              "text": "ProductName is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Product Price",
                              "component": "ProductPrice",
                              "name": "ProductPriceLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "decimal",
                              "type": "numeric",
                              "name": "ProductPrice",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${product.ProductPrice}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ProductPriceRequiredValidator",
                              "component": "ProductPrice",
                              "text": "ProductPrice is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Product Picture",
                              "component": "ProductPicture",
                              "name": "ProductPictureLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "file-input",
                              "name": "ProductPicture",
                              "style": {
                                "display": "block"
                              },
                              "value": "${product.ProductPicture}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ProductPictureRequiredValidator",
                              "component": "ProductPicture",
                              "text": "ProductPicture is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Product!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Sample.createProduct",
                        "parameters": [
                          {
                            "name": "Product",
                            "value": "${product}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${product}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${product}",
                  "visible": "${product != null}",
                  "itemType": "BlazorCustomPaging.Models.Sample.Product"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "product",
        "type": "set",
        "value": "new BlazorCustomPaging.Models.Sample.Product(){}",
        "valueType": "BlazorCustomPaging.Models.Sample.Product"
      }
    ]
  },
  "name": "Add Product",
  "layout": "Main"
}