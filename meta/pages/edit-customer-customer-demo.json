{
  "access": "Authenticated",
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "data": "${customercustomerdemo}",
                  "events": {
                    "cancel": [
                      {
                        "type": "back"
                      }
                    ],
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update CustomerCustomerDemo",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Northwind.updateCustomerCustomerDemo",
                        "parameters": [
                          {
                            "name": "CustomerID",
                            "value": "${parameters.CustomerID}"
                          },
                          {
                            "name": "CustomerTypeID",
                            "value": "${parameters.CustomerTypeID}"
                          },
                          {
                            "name": "CustomerCustomerDemo",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "loadData": [
                      {
                        "name": "Northwind.getCustomers",
                        "condition": "event.property == 'CustomerID'",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "${event.top}"
                          },
                          {
                            "name": "$skip",
                            "value": "${event.skip}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$count",
                            "value": "true"
                          }
                        ],
                        "then": [
                          {
                            "name": "getCustomersResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getCustomersCount",
                            "type": "set",
                            "value": "result['@odata.count']"
                          }
                        ],
                        "type": "invoke"
                      },
                      {
                        "name": "Northwind.getCustomerDemographics",
                        "condition": "event.property == 'CustomerTypeID'",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "${event.top}"
                          },
                          {
                            "name": "$skip",
                            "value": "${event.skip}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$count",
                            "value": "true"
                          }
                        ],
                        "then": [
                          {
                            "name": "getCustomerDemographicsResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getCustomerDemographicsCount",
                            "type": "set",
                            "value": "result['@odata.count']"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "fields": [],
                  "name": "form0",
                  "type": "form"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "Northwind.getCustomerCustomerDemoById",
        "parameters": [
          {
            "name": "CustomerID",
            "value": "${parameters.CustomerID}"
          },
          {
            "name": "CustomerTypeID",
            "value": "${parameters.CustomerTypeID}"
          }
        ],
        "then": [
          {
            "name": "customercustomerdemo",
            "type": "set",
            "value": "${result}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getCustomers",
        "parameters": [
          {
            "name": "$top",
            "value": "(<any>this.form0.fields.find(f => f.property == 'CustomerID')).pageSize"
          },
          {
            "name": "$skip",
            "value": "0"
          },
          {
            "name": "$count",
            "value": "true"
          }
        ],
        "then": [
          {
            "name": "getCustomersResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getCustomersCount",
            "type": "set",
            "value": "result['@odata.count']"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getCustomerDemographics",
        "parameters": [
          {
            "name": "$top",
            "value": "(<any>this.form0.fields.find(f => f.property == 'CustomerTypeID')).pageSize"
          },
          {
            "name": "$skip",
            "value": "0"
          },
          {
            "name": "$count",
            "value": "true"
          }
        ],
        "then": [
          {
            "name": "getCustomerDemographicsResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getCustomerDemographicsCount",
            "type": "set",
            "value": "result['@odata.count']"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Customer Customer Demo",
  "layout": "Main"
}